{
    "name": "teckhouse/analytics-bundle",
    "type": "symfony-bundle",
    "description": "Extensible stats system for your site.",
    "keywords": ["stat", "stats", "web stats", "stats", "analytics", "analysis", 
                "counter", "gauges", "graphs", "bars", "boxes", "leaderboard",
                "pins", "status", "timeline", "leaderboard", "funnles", "completition" ],
    "homepage": "https://github.com/TeckHouse/AnalyticsBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Mauro Foti",
            "email": "m.foti@teckhouse.com"
        }
    ],
    "require": {
        "php": ">=5.3.2"
    },
    "require-dev": {
        "symfony/framework-bundle": ">=2.1,<2.2-dev"
    },
    "suggest": {
        "knplabs/knp-menu-bundle": "Useful to show a menu with all available widget"
    },
    "autoload": {
        "psr-0": { "TeckHouse\\AnalyticsBundle": "" }
    },
    "minimum-stability": "dev",
    "target-dir": "TeckHouse/AnalyticsBundle"
}