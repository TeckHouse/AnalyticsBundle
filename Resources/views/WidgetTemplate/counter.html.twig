{% block teckhouse_analytics_widget_content %}
    Widget name: {{ widget.name }}
    <br />
    <br />
    Widget Values:
    {% for collection in widget.collections %}
        <br />Collection: {{ collection.collectionName }}
        {% for data in collection.collectionData %}
            <br /> @{{ data.timestamp|date('U') }}: {{ data.value }} 
        {% endfor %}
    {% endfor %}
        
{% endblock teckhouse_analytics_widget_content %}